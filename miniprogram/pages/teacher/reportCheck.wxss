/* miniprogram/pages/teacher/reportCheck.wxss */
page{background-color:#f8f3f3;}
.reportList{width:710rpx; background-color: white;border-radius: 20rpx;margin:20rpx auto;padding-bottom: 40rpx;}

.userBox{width:670rpx;height:100rpx;margin:0 auto;}
.user{height:100rpx;line-height:100rpx;float:left;font-size:28rpx;}

.titleBox{width:670rpx;margin:0 auto;height:100rpx;border-top:1px solid #f8f3f3;}
.title{width:670rpx;height:100rpx;line-height:100rpx;font-size:32rpx;float:left}
.time{height:50rpx;float:right;color:#808080;line-height:100rpx;}
.detailBox{width:670rpx;margin:0 auto;min-height:50rpx;}
.detail{line-height:50rpx;font-size:26rpx;text-indent: 2em;}

.buttonList{width:600rpx;height:60rpx;margin:20rpx auto 0;padding-top:20rpx;border-top:1px solid #f8f3f3;}
.okBtn{width:200rpx;height:60rpx;color:white;line-height:60rpx;font-size:28rpx;margin:0 auto;background-color: rgb(88, 187, 88);}
.okBtned{width:200rpx;height:60rpx;color:rgb(218, 214, 214);line-height:60rpx;font-size:28rpx;margin:0 auto;background-color: rgb(128, 153, 128);}

.emptyBox{width:100%}
.emptyBox image{width:400rpx;height:250rpx;margin:100rpx auto;display:block}
.emptyBox text{width:750rpx;line-height:100rpx;text-align:center;font-size:32rpx;color:#808080;display:block}